DROP TABLE C_SNTable;
CREATE TABLE C_SNTable as
SELECT *
FROM BMO_SNTable
UNION ALL
SELECT *
FROM CIBC_SNTable
UNION ALL
SELECT *
FROM PCF_SNTable
UNION ALL
SELECT *
FROM RBC_SNTable
UNION ALL
SELECT *
FROM Scotia_SNTable
UNION ALL
SELECT *
FROM Tangerine_SNTable
UNION ALL
SELECT *
FROM TD_SNTable
;
