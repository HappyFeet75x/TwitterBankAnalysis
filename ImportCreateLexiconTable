hadoop fs -rm /user/lab/LexiconV2.csv
hadoop fs -put LexiconV2.csv /user/lab
hadoop fs -cat /user/lab/LexiconV2.csv | head -n 2

DROP TABLE lex_table;
CREATE TABLE lex_table(
Word STRING,
Polarity STRING,
Type STRING,
PolType STRING
)
ROW FORMAT DELIMITED FIELDS TERMINATED BY ","
STORED AS TEXTFILE;
LOAD DATA INPATH '/user/lab/LexiconV2.csv' OVERWRITE INTO TABLE lex_table;
