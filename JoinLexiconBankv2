DROP TABLE Banks_lwc;
CREATE TABLE
Banks_lwc AS
SELECT A.word, A.cnt, B.Type, B.Polarity, B.PolType
FROM 
Banks_wc A
INNER JOIN
lex_table B
ON (A.word = B.Word)
;

