DROP TABLE BMO_ATable;
CREATE TABLE
BMO_ATable AS
SELECT * FROM BMO_Table WHERE LOWER(screen_name) NOT LIKE '%bank%'
AND LOWER(screen_name) NOT LIKE '%bmo%'
AND LOWER(screen_name) NOT LIKE '%cibc%'
AND LOWER(screen_name) NOT LIKE '%deals%'
AND LOWER(screen_name) NOT LIKE '%employ%'
AND LOWER(screen_name) NOT LIKE '%hotel%'
AND LOWER(screen_name) NOT LIKE '%job%'
AND LOWER(screen_name) NOT LIKE '%news%'
AND LOWER(screen_name) NOT LIKE '%ontario%'
AND LOWER(screen_name) NOT LIKE '%pcf%'
AND LOWER(screen_name) NOT LIKE '%rbc%'
AND LOWER(screen_name) NOT LIKE '%scotia%'
AND LOWER(screen_name) NOT LIKE '%tangerine%'
AND LOWER(screen_name) NOT LIKE '%td%'
AND LOWER(screen_name) NOT LIKE '%tmj%'
AND LOWER(screen_name) NOT LIKE '%invest%'
AND LOWER(screen_name) NOT LIKE '%ticker%'
AND LOWER(screen_name) NOT LIKE '%rating%'
AND LOWER(screen_name) NOT LIKE '%market%'
AND LOWER(screen_name) NOT LIKE '%financ%'
AND LOWER(screen_name) NOT LIKE '%watchlist%'
AND LOWER(screen_name) NOT LIKE '%mosaicguys%'
AND LOWER(screen_name) NOT LIKE '%money%'
AND LOWER(screen_name) NOT LIKE '%consumerfeed%'
AND LOWER(screen_name) NOT LIKE '%mktg%'
AND LOWER(screen_name) NOT LIKE '%intern%'
AND LOWER(screen_name) NOT LIKE '%expert%'
AND LOWER(screen_name) NOT LIKE '%analyst%'
;

